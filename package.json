{
  "name": "pdf-generator",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "copy-files": "copyfiles -u 1 src/public/**/*.* src/public/*.* dist/src/",
    "build": "npm run clean && tsc && npm run copy-files",
    "start": "node ./dist/src/index.js",
    "clean": "rimraf ./dist",
    "local": "ts-node src",
    "local:watch": "nodemon ./src -e ts,json,html --exec 'npm run local'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.11",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "express": "^4.19.2",
    "mongodb": "^6.6.1",
    "pdf-lib": "^1.17.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  }
}
